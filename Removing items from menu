Sub 卵アレルギーチェック()
'
' 卵アレルギーチェック Macro
'

'
    Sheets("卵").Select
    ActiveSheet.Range("$B$2:$C$51").AutoFilter Field:=1, Criteria1:="-"
    Range("B2:C49").Select
    Selection.Copy
    Sheets("結果").Select
    Range("B5").Select
    ActiveSheet.Paste
End Sub
