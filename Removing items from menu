Sub かにアレルギーチェック()
'
' かにアレルギーチェック Macro
'

'
    Sheets("かに").Select
    Range("B2").Select
    Application.CutCopyMode = False
    Selection.AutoFilter
    ActiveSheet.Range("$B$2:$C$51").AutoFilter Field:=1
    Range("B2:C51").Select
    Selection.Copy
    Sheets("結果").Select
    ActiveWindow.SmallScroll Down:=74
    Range("B174").Select
    ActiveSheet.Paste
End Sub
